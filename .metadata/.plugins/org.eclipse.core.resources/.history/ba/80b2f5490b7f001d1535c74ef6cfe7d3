#include <ctype.h>
#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>
#include <termios.h>
#include <unistd.h>
#include <sys/ioctl.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>
#include <string.h>
#include "Serial.h"
#include "Modbus.h"

#define	VOLTAGE 		0
#define CURRENT			1
#define ACTIVEPOWER		2



double ConvertToRegisterUnits(int data, int type)
{
	double ConvertedData;
	if (type == VOLTAGE)
	{
		ConvertedData = ((data*10)/100);
		return ConvertedData;
	}
	return 0.0;
}
